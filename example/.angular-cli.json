{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "demo-handy-angular-components"
  },
  "apps": [
    {
      "name": "demo-jit",
      "root": "demo",
      "outDir": "./dist",
      "assets": [
      ],
      "index": "index.html",
