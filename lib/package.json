{
  "name": "handy-angular-components",
  "version": "0.6.0-beta1",
  "scripts": {
    "build": "ng build --prod",
    "v1gulp": "gulp",
    "v1build": "gulp build",
    "v1build:watch": "gulp",
    "v1docs": "npm run docs:build",
    "v1docs:build": "compodoc -p tsconfig.json -n handy-angular-components -d docs --hideGenerator",
    "v1docs:serve": "npm run docs:build -- -s",
    "v1docs:watch": "npm run docs:build -- -s -w",
    "v1lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "v1test": "tsc && karma start",
    "v1dist:dev": "cpx \"./src/**/*\" \"../dist/\" -w",
    "v1predev": "rimraf ../dist",
    "v1dev": "concurrently \"npm run dist:dev\" \"gulp watch:styles\" --names \"NG,ASSETS\" --prefix name",
    "ng": "ng"
  },
  "repository": {
    "type": "git",
