    "protractor": "~5.1.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "run-sequence": "^1.2.2",
    "rxjs": "^5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1",
    "zone.js": "^0.8.4"
