{
  "name": "handy-angular-components",
  "version": "0.2.2",
  "scripts": {
    "gulp": "gulp",
    "build": "gulp build",
    "build:watch": "gulp",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n handy-angular-components -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "test": "tsc && karma start",
    "dist:dev": "cpx \"./src/**/*\" \"../dist/\" -w",
    "predev": "rimraf ../dist",
    "dev": "concurrently \"npm run dist:dev\" \"gulp watch:styles\" --names \"NG,ASSETS\" --prefix name"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joanjane/handy-angular-components"
  },
  "author": {
    "name": "Joan JanÃ©",
    "email": "jjaneballester@gmail.com"
  },
  "keywords": [
    "angular"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://joanjane.visualstudio.com/Experiments/_git/handy-angular-components/issues"
  },
  "devDependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@compodoc/compodoc": "^1.0.0-beta.7",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "^3.1.2",
    "concurrently": "^3.5.0",
    "core-js": "^2.4.1",
    "cpx": "^1.5.0",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-clean-css": "^3.6.0",
    "gulp-rename": "^1.2.2",
    "gulp-rollup": "^2.11.0",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-wait": "0.0.2",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
